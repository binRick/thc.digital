    "express": "4.6.1",
    "morgan": "1.2.0",
    "node-options": "0.0.6",
    "socket.io": "1.0.6",
    "winston": "0.7.3"
  },
  "engines": {
    "node": ">= 0.8.0 < 0.11.0"
  },
  "main": "./lib/tomahawk.js",
  "bin": {
    "tomahawk": "./bin/tomahawk"
  },
  "scripts": {
    "start": "bin/tomahawk"
